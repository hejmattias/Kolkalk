<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleDevelopmentRegion</key>
    <string>en</string>
    <key>CFBundleExecutable</key>
    <string>$(EXECUTABLE_NAME)</string>
    <key>CFBundleIdentifier</key>
    <string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>
    <key>CFBundleName</key>
    <string>$(PRODUCT_NAME)</string>
    <key>CFBundleDisplayName</key>
    <string>Kolkalk</string>
    <key>CFBundlePackageType</key>
    <string>APPL</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0</string> <!-- Ändra vid release -->
    <key>CFBundleVersion</key>
    <string>1</string>   <!-- Öka för varje build -->
    <key>LSRequiresIPhoneOS</key>
    <true/>
    <key>UIBackgroundModes</key>
    <array>
        <string>fetch</string>
        <string>bluetooth-central</string>
        <string>remote-notification</string>
    </array>
    <key>UIFileSharingEnabled</key>
    <true/>
    <!-- Om du använder UIKit-storyboard, avkommentera nästa två rader och justera vid behov -->
    <!--
    <key>UIMainStoryboardFile</key>
    <string>Main</string>
    -->
    <!-- Scene-manifest krävs för SwiftUI och iOS 13+ -->
    <key>UIApplicationSceneManifest</key>
    <dict>
        <key>UIApplicationSupportsMultipleScenes</key>
        <false/>
        <key>UISceneConfigurations</key>
        <dict>
            <key>UIWindowSceneSessionRoleApplication</key>
            <array>
                <dict>
                    <key>UISceneConfigurationName</key>
                    <string>Default Configuration</string>
                    <key>UISceneDelegateClassName</key>
                    <string>$(UI_SCENE_DELEGATE_CLASS)</string>
                </dict>
            </array>
        </dict>
    </dict>
    <!-- Om du använder App Groups (för Widget/Watch/komplikation): -->
    <!--
    <key>com.apple.security.application-groups</key>
    <array>
        <string>group.mg.kolkalk</string>
    </array>
    -->
    <!-- CloudKit-användning (lägg till om du laddar upp till App Store/aktiverar CloudKit i Capabilities) -->
    <key>NSUbiquitousContainers</key>
    <dict>
        <key>iCloud.MG.kolkylator</key>
        <dict>
            <key>NSUbiquitousContainerIsDocumentScopePublic</key>
            <true/>
            <key>NSUbiquitousContainerName</key>
            <string>Kolkalk</string>
            <key>NSUbiquitousContainerSupportedFolderLevels</key>
            <string>Any</string>
        </dict>
    </dict>
    <!-- Privacy-strängar: krävs för App Store om du använder hälsa, kamera, bilder etc -->
    <!--
    <key>NSHealthShareUsageDescription</key>
    <string>För att kunna logga kolhydrater och insulin till Apple Hälsa.</string>
    <key>NSHealthUpdateUsageDescription</key>
    <string>För att kunna logga kolhydrater och insulin till Apple Hälsa.</string>
    <key>NSPhotoLibraryUsageDescription</key>
    <string>Behövs för att kunna välja bilder.</string>
    <key>NSCameraUsageDescription</key>
    <string>Behövs för att kunna ta bilder till appen.</string>
    -->
</dict>
</plist>
