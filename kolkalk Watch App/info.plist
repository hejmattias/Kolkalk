<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<!-- Info.plist for kolkalk Watch App -->
<plist version="1.0">
<dict>
    <!-- App Information -->
    <key>CFBundleDisplayName</key>
    <string>kolkalk</string>
    <key>CFBundleIdentifier</key>
    <string>MG.Kolkalk.watchkitapp</string>
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>
    <key>CFBundleName</key>
    <string>kolkalk</string>
    <key>CFBundlePackageType</key>
    <string>APPL</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0</string>
    <key>CFBundleVersion</key>
    <string>1</string>

    <!-- WatchKit Extension Delegate -->
    <key>WKExtensionDelegateClassName</key>
    <string>MG.Kolkalk.watchkitapp.ExtensionDelegate</string>

    <!-- Background Modes -->
    <key>UIBackgroundModes</key>
    <array>
        <string>remote-notification</string>
        <string>fetch</string>
        <string>watch-connectivity</string>
    </array>

    <!-- Custom URL Scheme -->
    <key>CFBundleURLTypes</key>
    <array>
        <dict>
            <key>CFBundleURLName</key>
            <string>MG.Kolkalk.watchkitapp</string> <!-- Replace with your actual Bundle Identifier -->
            <key>CFBundleURLSchemes</key>
            <array>
                <string>kolkalk</string>
            </array>
        </dict>
    </array>

    <!-- Required Device Capabilities -->
    <key>UIRequiredDeviceCapabilities</key>
    <array>
        <string>watch-companion</string>
    </array>

    <!-- Other configurations (if any) -->
    <!-- ... -->
</dict>
</plist>
